{
  "entries": [
    {
      "entry": "./lib/webparts/contacts/ContactsWebPart.js",
      "manifest": "./src/webparts/contacts/ContactsWebPart.manifest.json",
      "outputPath": "./dist/contacts.bundle.js"
    },
    {
      "entry": "./lib/webparts/reactContacts/ReactContactsWebPart.js",
      "manifest": "./src/webparts/reactContacts/ReactContactsWebPart.manifest.json",
      "outputPath": "./dist/react-contacts.bundle.js"
